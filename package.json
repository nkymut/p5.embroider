{
  "name": "p5.embroider",
  "version": "0.1.0",
  "description": "P5.js extension for embroidery pattern generation",
  "main": "lib/p5.embroider.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "jsdoc": "jsdoc -c jsdoc.config.json",
    "serve-docs": "npx http-server ./docs -o",
    "codeformat": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "p5": "^1.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "clean-jsdoc-theme": "^4.2.17",
    "glob": "^10.4.5",
    "jsdoc": "^4.0.2",
    "prettier": "^3.2.5",
    "rollup": "^4.12.0"
  },
  "type": "module"
}
